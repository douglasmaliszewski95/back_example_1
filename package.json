{
  "name": "incredbull-core-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsup src --out-dir public",
    "dev": "tsx watch src/main.ts",
    "start": "node build/main.js",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint",
    "vercel-build": "npm run build && npx prisma generate",
    "seed": "npx prisma db seed",
    "prepare": "husky",
    "ts-check": "npx tsc --noEmit",
    "pre-commit": "npm run ts-check && npm run lint",
    "pre-push": "npm run test"
  },
  "prisma": {
    "seed": "tsx prisma/seed/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "3.577.0",
    "@fastify/caching": "8.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "8.2.0",
    "@fastify/swagger": "8.14.0",
    "@fastify/swagger-ui": "3.0.0",
    "@prisma/client": "5.14.0",
    "@supabase/supabase-js": "2.43.2",
    "ethers": "^6.13.4",
    "@vercel/node": "3.2.1",
    "axios": "1.7.2",
    "csv-parser": "3.0.0",
    "dotenv": "16.4.7",
    "exceljs": "4.4.0",
    "fastify": "4.27.0",
    "image-size": "1.1.1",
    "log4js": "6.9.1",
    "node-pushnotifications": "^3.1.1",
    "prisma": "5.14.0",
    "uuid": "10.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.12",
    "@types/node-pushnotifications": "^1.0.8",
    "@types/uuid": "9.0.8",
    "eslint": "7.0.0",
    "husky": "^9.1.4",
    "tsup": "8.0.2",
    "tsx": "4.10.5",
    "typescript": "5.4.5",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  }
}
